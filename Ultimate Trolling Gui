script.Parent.MouseButton1Click:Connect(function()
frames.Text.TabName.Text = "Home"
frames.Tab1.Visible = false
frames.Tab2.Visible = false
frames.Tab3.Visible = false
frames.Tab4.Visible = false
frames.Tab5.Visible = false
frames.Tab6.Visible = false
frames.Home.Visible = true
selection.Tab1.Visible = false
selection.Tab2.Visible = false
selection.Tab3.Visible = false
selection.Tab4.Visible = false
selection.Tab5.Visible = false
selection.Tab6.Visible = false
selection.Home.Visible = true
end)
script.Parent.MouseEnter:Connect(function()
script.Parent.TextSize = 23
end)
script.Parent.MouseLeave:Connect(function()
script.Parent.TextSize = 18
end)
script.Parent.MouseButton1Down:Connect(function()
script.Parent.TextSize = 15
end)
script.Parent.MouseButton1Up:Connect(function()
script.Parent.TextSize = 18
end)

end
coroutine.resume(coroutine.create(SCRIPT_LJEZ68_FAKESCRIPT))
function SCRIPT_ZCUD65_FAKESCRIPT() -- Main.LocalScript
local script = Instance.new('LocalScript')
script.Parent = Main
local main = script.Parent
main.Active = true
main.Selectable = true
main.Draggable = true

end
coroutine.resume(coroutine.create(SCRIPT_ZCUD65_FAKESCRIPT))
function SCRIPT_FPHH74_FAKESCRIPT() -- Close.LocalScript
local script = Instance.new('LocalScript')
script.Parent = Close
script.Parent.MouseEnter:Connect(function()
script.Parent.TextSize = 23
end)
script.Parent.MouseLeave:Connect(function()
script.Parent.TextSize = 18
end)
script.Parent.MouseButton1Down:Connect(function()
script.Parent.TextSize = 15
end)
script.Parent.MouseButton1Up:Connect(function()
script.Parent.TextSize = 18
end)

end
coroutine.resume(coroutine.create(SCRIPT_FPHH74_FAKESCRIPT))
function SCRIPT_STJY80_FAKESCRIPT() -- Open.LocalScript
local script = Instance.new('LocalScript')
script.Parent = Open
script.Parent.MouseEnter:Connect(function()
script.Parent.TextSize = 23
end)
script.Parent.MouseLeave:Connect(function()
script.Parent.TextSize = 18
end)
script.Parent.MouseButton1Down:Connect(function()
script.Parent.TextSize = 15
end)
script.Parent.MouseButton1Up:Connect(function()
script.Parent.TextSize = 18
end)

end
coroutine.resume(coroutine.create(SCRIPT_STJY80_FAKESCRIPT))
function SCRIPT_HULN88_FAKESCRIPT() -- Trollplayergui.LocalScript
local script = Instance.new('LocalScript')
script.Parent = Trollplayergui
local close = script.Parent.Main.Close
local open = script.Parent.OpenFrame.Open
close.MouseButton1Click:Connect(function()
script.Parent.Main.Visible = false
script.Parent.OpenFrame.Visible = true
end)
open.MouseButton1Click:Connect(function()
script.Parent.Main.Visible = true
script.Parent.OpenFrame.Visible = false
end)



end
coroutine.resume(coroutine.create(SCRIPT_HULN88_FAKESCRIPT))
end)

