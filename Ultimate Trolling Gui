-- Scripts:
function SCRIPT_UGWK84_FAKESCRIPT() -- Tab1_2.LocalScript
local script = Instance.new('LocalScript')
script.Parent = Tab1_2
local frames = script.Parent.Parent.Parent
local selection = script.Parent.Parent.Selection
script.Parent.MouseButton1Click:Connect(function()
frames.Tab1.Visible = true
frames.Text.TabName.Text = "Gui"
frames.Tab2.Visible = false
frames.Tab3.Visible = false
frames.Tab4.Visible = false
frames.Tab5.Visible = false
frames.Tab6.Visible = false
frames.Home.Visible = false
selection.Tab1.Visible = true
selection.Tab2.Visible = false
selection.Tab3.Visible = false
selection.Tab4.Visible = false
selection.Tab5.Visible = false
selection.Tab6.Visible = false
selection.Home.Visible = false
end)
script.Parent.MouseEnter:Connect(function()
script.Parent.TextSize = 23
end)
script.Parent.MouseLeave:Connect(function()
script.Parent.TextSize = 18
end)
script.Parent.MouseButton1Down:Connect(function()
script.Parent.TextSize = 15
end)
script.Parent.MouseButton1Up:Connect(function()
script.Parent.TextSize = 18
end)

end
coroutine.resume(coroutine.create(SCRIPT_UGWK84_FAKESCRIPT))
function SCRIPT_LJEZ68_FAKESCRIPT() -- Home_2.LocalScript
local script = Instance.new('LocalScript')
script.Parent = Home_2
